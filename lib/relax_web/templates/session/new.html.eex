<%= form_for @conn, Routes.session_path(@conn, :create), [as: :session_params],  fn f -> %>
  <%= label f, :name%>
  <%= text_input f, :name%>
  <%= error_tag f, :name%>
  <%= label f, :password%>
  <%= password_input f, :password%>
  <%= error_tag f, :password%>
  <%= submit "Login" %>
<% end %>

<a href="<%= Routes.user_path(RelaxWeb.Endpoint, :new)  %>">Signup instead</a>
